generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model startup {
  id                Int    @id
  name              String @db.VarChar(50)
  sector            String @db.VarChar(50)
  marketCap         Int
  activeYears       Int
  region            String @db.VarChar(20)
  numberOfInvestors Int
}
